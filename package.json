{
  "name": "warehouse-service",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "npm run clean && tsoa spec-and-routes && npx tsc",
    "clean": "rimraf dist",
    "start": "node  --experimental-specifier-resolution=node dist/server.js",
    "dev": "npm run build && concurrently \"npx tsc --watch\" \"nodemon --experimental-specifier-resolution=node -q dist/server.js\"",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint": "concurrently \"npm run lint:check\" \"npm run format:check\"",
    "fix": "npm run lint:fix && npm run format:write",
    "test": "jest --coverage --verbose"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "concurrently": "^8.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@tsoa/runtime": "^5.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "rimraf": "^5.0.0",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^10.9.1",
    "tsoa": "^5.1.1",
    "zod": "^3.21.4"
  }
}
